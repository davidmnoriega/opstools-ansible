{
  "client": {
    "name": "{{ sensu_client_name }}",
    "address": "{{ sensu_client_address }}",
    "subscriptions": [
      "{{ sensu_client_subscription }}"
    ],
    "socket": {
      "bind": "{{ sensu_client_bind }}",
      "port": {{ sensu_client_port }}
    },
    "openstack": {
      "auth_url": "{{ openstack_auth_url }}",
      "password": "{{ openstack_password }}",
      "project_name": "{{ openstack_project_name }}",
      "username": "{{ openstack_username }}",
      "user_domain_name": "{{ openstack_user_domain_name }}",
      "project_domain_name": "{{ openstack_project_domain_name }}",
      "compute_api_version": "{{ openstack_compute_api_version }}",
      "image_api_version": "{{ openstack_image_api_version }}",
      "volume_api_version": "{{ openstack_volume_api_version }}",
      "identity_api_version": "{{ openstack_identity_api_version }}"
    },
    "redact": [
      "auth",
      "password"
    ],
    "haproxy": {
     "auth": "{{ haproxy_data.stdout_lines[1] }}",
     "uristats": "{{ haproxy_data.stdout_lines[0] }}"
    },
    "sensu":{
     "api": "http://{{ sensu_api_server }}:{{ sensu_api_port }}"
    }
  }
}
